{
  "name": "did-method-key",
  "version": "0.6.2-0",
  "description": "A did:key method driver for did-io and standalone use.",
  "homepage": "http://github.com/digitalbazaar/did-method-key",
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/did-method-key"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/did-method-key/issues",
    "email": "support@digitalbazaar.com"
  },
  "license": "BSD-3-Clause",
  "main": "lib/",
  "dependencies": {
    "crypto-ld": "^3.7.0",
    "security-context": "^4.0.0",
    "x25519-key-pair": "digitalbazaar/x25519-key-pair#wasm"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-digitalbazaar": "^2.3.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "keywords": [
    "Decentralized",
    "Identifier",
    "DID"
  ],
  "scripts": {
    "test": "npm run test-node",
    "test-node": "cross-env NODE_ENV=test mocha --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} tests/*.spec.js",
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "lint": "eslint lib tests",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=text-lcov npm run test-node > coverage.lcov",
    "coverage-report": "nyc report"
  }
}
