{
  "name": "@digitalbazaar/did-method-key",
  "version": "1.1.1-0",
  "description": "A did:key method resolver.",
  "homepage": "http://github.com/digitalbazaar/did-method-key-js",
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/did-method-key-js"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/did-method-key-js/issues",
    "email": "support@digitalbazaar.com"
  },
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "lib/main.js",
  "dependencies": {
    "@digitalbazaar/did-io": "^1.0.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^2.1.1",
    "@digitalbazaar/x25519-key-agreement-key-2020": "^1.0.0",
    "did-context": "^3.0.0",
    "ed25519-signature-2020-context": "^1.0.1",
    "esm": "^3.2.25",
    "x25519-key-agreement-2020-context": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-digitalbazaar": "^2.6.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "karma": "^6.3.1",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "util": false
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "Decentralized",
    "Identifier",
    "DID"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-node && npm run test-karma",
    "test-node": "cross-env NODE_ENV=test mocha -r esm --preserve-symlinks -t 10000 test/*.spec.js",
    "test-karma": "karma start karma.conf.js",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=text-lcov npm run test-node > coverage.lcov",
    "coverage-report": "nyc report",
    "lint": "eslint ."
  }
